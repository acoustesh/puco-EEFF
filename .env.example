# puco-EEFF Environment Variables
# Copy this file to .env and fill in your API keys

# Mistral AI - Primary OCR
MISTRAL_API_KEY=your_mistral_api_key_here

# Anthropic - Fallback OCR via OpenRouter
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenRouter - For fallback OCR (Anthropic + OpenAI)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# OpenAI - Fallback OCR via OpenRouter
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Custom paths (defaults shown)
# DATA_DIR=./data
# AUDIT_DIR=./audit
# LOGS_DIR=./logs
